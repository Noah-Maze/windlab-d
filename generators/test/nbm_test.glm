};

object climate{
	name MyClimate;
	tmyfile WA-Seattle.tmy2;
	//reader CsvReader;
	object recorder {
		file climate_out.csv;
		interval 1800;
		limit 60;
		property temperature,wind_speed;
	};
};

object windclass {
	name test_wdg;
	object recorder {
		file WT_out.csv;
		interval 900;
		limit 20000;
		property power_out;
	};
};
